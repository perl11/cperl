            PL_laststatval = PerlLIO_stat(fn, &PL_statcache);
	    if (!S_ISDIR(PL_statcache.st_mode)) {
                PerlIO_printf(Perl_debug_log, " dl_findfile ignored non-existent directory: %s\n",
                                fn);
